{
    "presets": [
        "next/babel"
    ],
    "ignore": ["node_modules"],
    "plugins": [
        [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory":"lib",
                "style": true
            },
            
        ],
        ["module-resolver", {
            "alias": {
              "atoms": "./src/components/atoms",
              "molecules":"./stc/components/molecules",
              "components":"./src/components"
            }
        }]
    ]
}
